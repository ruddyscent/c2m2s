Cell[TextData[{
 "A set ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],ExpressionUUID->
  "425cccf0-6c3f-454d-912a-aa8804782539"],
 " spans a vector space ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "4f7d4bfc-3973-4172-9a4a-e57304bace16"],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ForAll]", 
    RowBox[{"v", "\[Element]", "V"}]}], TraditionalForm]],ExpressionUUID->
  "6c2d4974-73c0-4d7e-9a4e-b70e6880663a"],
 " can be represented as a linear combination of s\[Element]S."
}], "Text",
 CellChangeTimes->{{3.725937154460026*^9, 
  3.725937234426345*^9}},ExpressionUUID->"88339217-2f8e-403a-8fe5-\
b9c050a0db3e"],

Cell[TextData[{
 "A set ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],ExpressionUUID->
  "1f84e50d-c033-4797-a63f-d95a197113db"],
 " is a basis for a vector space ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "915e48f0-9f60-48dd-af56-e1fbabb95318"],
 ", if\n1. ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],ExpressionUUID->
  "f4c9ee96-3d3a-40d6-88d8-538721a1a8a9"],
 " spans ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "bbc1dee9-8be1-4357-861b-7a423b511b1a"],
 ".\n2. ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],ExpressionUUID->
  "24f10644-f6fc-4343-9b86-cea9433b519b"],
 " is linearly independent."
}], "Text",
 CellChangeTimes->{{3.725937237352304*^9, 
  3.7259372785405073`*^9}},ExpressionUUID->"c9c82784-3afa-473e-9e2b-\
50ad2bff37bc"],

Cell[TextData[{
 "The dimension of a vector space ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "2700af24-c06a-4352-a680-6f8910498299"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"dim", "[", "V", "]"}], TraditionalForm]],ExpressionUUID->
  "7523058c-1ce5-440d-b488-50348bb38c4a"],
 ", is the number of vectors in a basis."
}], "Text",
 CellChangeTimes->{{3.725937398157453*^9, 
  3.7259374243314342`*^9}},ExpressionUUID->"3f6b2753-1380-4bda-8232-\
2cfc75ea718b"],

Cell[TextData[{
 "If the number of basis vectors for a vector space is finite we call the \
vector space ",
 StyleBox["finite dimensional",
  FontSlant->"Italic"],
 ", otherwise, ",
 StyleBox["infinite dimensional",
  FontSlant->"Italic"],
 "."
}], "Text",

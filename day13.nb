Cell[TextData[{
 "Exercises: Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{"V", "=", 
    SuperscriptBox["R", "2"]}], TraditionalForm]],ExpressionUUID->
  "d2f11832-c0e1-40c1-a9d1-5b78cf618c58"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["u", "1"], ",", 
      SubscriptBox["u", "2"]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "4a37f65b-0cb8-4750-9445-003e5850dd9b"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"v", "=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["v", "1"], ",", 
      SubscriptBox["v", "2"]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "a05b2248-7d00-4313-bd95-7c829f1612f6"],
 " in ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "e24d6c8e-7569-4b41-9884-9ab09d994b82"],
 ". Define addition as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"u", "+", "v"}], "=", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SubscriptBox["u", "1"], "+", 

Cell[TextData[{
 "An ",
 StyleBox["inner product",
  FontSlant->"Italic"],
 " on set V is a function that maps ordered pairs ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"x", ",", "y"}], ")"}], TraditionalForm]],ExpressionUUID->
  "d4ff9272-c173-4fbf-9c47-763454498af5"],
 " from ",
 Cell[BoxData[
  FormBox[
   RowBox[{"V", "\[Times]", "V"}], TraditionalForm]],ExpressionUUID->
  "1d857b77-2375-42ed-a80c-e15230575a8e"],
 " to a number ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{"x", ",", "y"}], "\[RightAngleBracket]"}], TraditionalForm]],
  ExpressionUUID->"1af8ce74-1ffa-494e-be50-6747132a6080"],
 " while satisfying the following properties:\n1. ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ForAll]", 
    RowBox[{
     OverscriptBox["v", "\[RightVector]"], "\[Element]", "V"}]}], 
   TraditionalForm]],ExpressionUUID->"cd1e2656-b3c3-41a2-9205-af3583f1715c"],
 ",  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      OverscriptBox["v", "\[RightVector]"], ",", 
      OverscriptBox["v", "\[RightVector]"]}], "\[RightAngleBracket]"}], 
    "\[GreaterEqual]", "0"}], TraditionalForm]],ExpressionUUID->
  "b1689da8-01ba-43f9-8298-7e0a2d4d152a"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      OverscriptBox["v", "\[RightVector]"], ",", 
      OverscriptBox["v", "\[RightVector]"]}], "\[RightAngleBracket]"}], "=", 
    "0"}], TraditionalForm]],ExpressionUUID->
  "25e82c82-bf7a-4016-9514-d90a9ca1641e"],
 " iff ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["v", "\[RightVector]"], "=", 
    OverscriptBox["0", "\[RightVector]"]}], TraditionalForm]],ExpressionUUID->
  "91679ed4-cd07-4c17-81a8-5fc0a920695c"],
 ".\n2. ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ForAll]", "u"}], ",", "v", ",", 
    RowBox[{"w", "\[Element]", "V"}]}], TraditionalForm]],ExpressionUUID->
  "b7ab991b-78c5-4c9d-8a84-d7676cd5f6d6"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"u", ",", 
      RowBox[{"v", "+", "w"}]}], "\[RightAngleBracket]"}], "=", 
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"u", ",", "v"}], "\[RightAngleBracket]"}], "+", 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"u", ",", "w"}], "\[RightAngleBracket]"}]}]}], 
   TraditionalForm]],ExpressionUUID->"a25a3efd-5149-4daf-b1ec-7af24f27c99b"],
 ".\n3. ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ForAll]", "u"}], ",", 
    RowBox[{"v", "\[Element]", "V"}]}], TraditionalForm]],ExpressionUUID->
  "0aa5e2e3-a58f-4b5a-abd1-79888d9702b4"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", "\[Element]", "\[DoubleStruckCapitalZ]"}], TraditionalForm]],
  ExpressionUUID->"15d83c81-f5ef-4076-8d98-d7d20a33cd1a"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{"k", " ", "u"}], ",", "v"}], "\[RightAngleBracket]"}], "=", 
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"u", ",", 
       RowBox[{"k", " ", "v"}]}], "\[RightAngleBracket]"}], "=", 
     RowBox[{"k", 
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{"u", ",", "v"}], "\[RightAngleBracket]"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"499f3b34-363e-41fc-9c9c-b382ffc8d99a"],
 ".\n4. ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ForAll]", "u"}], ",", 
    RowBox[{"v", "\[Element]", "V"}]}], TraditionalForm]],ExpressionUUID->
  "866e0f0f-f0d5-40a3-9e9e-28509404a8db"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"u", ",", "v"}], "\[RightAngleBracket]"}], "=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"v", ",", "u"}], "\[RightAngleBracket]"}]}], TraditionalForm]],
  ExpressionUUID->"866fe49f-2462-4d80-aeb8-656d80ab6d35"],

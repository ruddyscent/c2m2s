Cell["More about Orthogonality", "Subsection",
 CellChangeTimes->{{3.7261755682944727`*^9, 
  3.7261755737097054`*^9}},ExpressionUUID->"4919defb-f7ee-4937-996c-\
4ae410df5bbc"],

Cell[TextData[{
 "A set is called ",
 StyleBox["orthogonal",
  FontSlant->"Italic"],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ForAll]", 
    RowBox[{
     RowBox[{"v", "\[NotEqual]", "w"}], "\[Element]", "V"}]}], 
   TraditionalForm]],ExpressionUUID->"540886c8-03ed-4655-bcd4-50c1c8e54eed"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"v", ",", "w"}], "\[RightAngleBracket]"}], "=", "0"}], 
   TraditionalForm]],ExpressionUUID->"3f2d208e-f807-474b-bda0-80c5a9b07db9"],
 "."
}], "Text",
 CellChangeTimes->{{3.726372721839142*^9, 
  3.726372798253422*^9}},ExpressionUUID->"bc82d5a0-f658-48b8-8ab7-\
b603f45c574e"],

Cell[TextData[{
 "In addition, if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ForAll]", 
    RowBox[{"v", "\[Element]", "V"}]}], TraditionalForm]],ExpressionUUID->
  "3e447ce2-6ff3-4425-81e8-a41305f8e8d3"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"v", ",", "v"}], "\[RightAngleBracket]"}], "=", "1"}], 
   TraditionalForm]],ExpressionUUID->"02e885e8-0d30-4aa3-9f42-fb712ac44826"],
 " then the set is called ",
 StyleBox["orthonormal",
  FontSlant->"Italic"],
 "."
}], "Text",
 CellChangeTimes->{{3.72637281389557*^9, 
  3.726372870511055*^9}},ExpressionUUID->"6fff2d37-b79d-4f6b-a66c-\
6fc238f4c55b"],

Cell[TextData[{
 "A basis for an orthonormal set is an ",
 StyleBox["orthonormal basis",
  FontSlant->"Italic"],
 "."
}], "Text",
 CellChangeTimes->{{3.7263728747493153`*^9, 
  3.726372907010724*^9}},ExpressionUUID->"ed2f9110-6fcc-4a84-85cb-\
002317b8d88c"],


Cell[TextData[{
 "A vector space with a defined inner product is called an ",
 StyleBox["inner product space",
  FontSlant->"Italic"],
 "."
}], "Text",
 CellChangeTimes->{{3.726370310602684*^9, 
  3.726370341235012*^9}},ExpressionUUID->"961bfabb-4707-455d-9e52-\
9b6cdcfc5f10"],

Cell[TextData[{
 "The ",
 StyleBox["Euclidean inner product",
  FontSlant->"Italic"],
 " a.k.a ",
 StyleBox["dot product",
  FontSlant->"Italic"],
 ", in ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["R", "n"], TraditionalForm]],ExpressionUUID->
  "00784f43-03ca-4d2a-a3eb-aa9c2703542f"],
 " is defined as\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{"u", ",", "v"}], "\[RightAngleBracket]"}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "1"}], "n"], 
     RowBox[{
      SubscriptBox["u", "i"], 
      SubscriptBox["v", "i"]}]}]}], TraditionalForm]],ExpressionUUID->
  "e7314fd9-6c89-4651-8210-d9b7e9b8cf16"],
 "\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "=", 
    RowBox[{"Table", "[", 
     RowBox[{
      SubscriptBox["u", "i"], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "n"}], "}"}]}], "]"}]}], TraditionalForm]],
  ExpressionUUID->"bd926c18-ebab-4d62-b4d4-d0b0e5124c60"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"v", "=", 
    RowBox[{"Table", "[", 
     RowBox[{
      SubscriptBox["v", "i"], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "n"}], "}"}]}], "]"}]}], TraditionalForm]],
  ExpressionUUID->"63fa8a92-53cc-4b95-b814-c763a0592956"],
 " are in ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["R", "n"], TraditionalForm]],ExpressionUUID->
  "83337bae-ab05-4a59-b106-05d92b098d89"],
 "."
}], "Text",
 CellChangeTimes->{{3.726370357876486*^9, 
  3.7263704936836023`*^9}},ExpressionUUID->"b9f4ccdc-ac30-40b4-b440-\
bca043d39bc1"],


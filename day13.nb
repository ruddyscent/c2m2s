Cell[TextData[{
 StyleBox["Theorem 69.",
  FontWeight->"Bold"],
 " If ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "dbc98e77-d04a-439c-9015-7c5df8270d19"],
 " is symmetric then any two eigenvectors of ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
